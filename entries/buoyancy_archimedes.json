  "derivation": [
    {
      "step": 1,
      "equation": "P = P_0 + \\rho g h"
    },
    {
      "step": 2,
      "equation": "F_{top} = P_{top} A = (P_0 + \\rho g h_{top}) A"
    },
    {
      "step": 3,
      "equation": "F_{bottom} = P_{bottom} A = (P_0 + \\rho g h_{bottom}) A"
    },
    {
      "step": 4,
      "equation": "F_b = F_{bottom} - F_{top} = \\rho g A (h_{bottom} - h_{top})"
    },
    {
      "step": 5,
      "equation": "V_{submerged} = A (h_{bottom} - h_{top})"
    },
    {
      "step": 6,
      "equation": "F_b = \\rho_{fluid} g V_{submerged}"
    }
