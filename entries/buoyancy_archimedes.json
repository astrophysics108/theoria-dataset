  "programmatic_verification": {
    "language": "python 3.11.12",
    "library": "sympy 1.12.0",
    "code": [
      "import math",
      "",
      "print('Archimedes Principle Verification\\n')",
      "print('=' * 40)",
      "",
      "# Test 1: Floating object equilibrium",
      "def verify_floating_object():",
      "    \"\"\"Verify conditions for floating objects\"\"\"",
      "    print('Test 1: Floating object equilibrium')",
      "    ",
      "    # Wood block in water",
      "    rho_water = 1000  # kg/m³",
      "    rho_wood = 600    # kg/m³ (typical for pine)",
      "    g = 9.81          # m/s²",
      "    ",
      "    # Block dimensions",
      "    length = 2.0   # m",
      "    width = 1.0    # m",
      "    height = 0.5   # m",
      "    V_total = length * width * height",
      "    ",
      "    # Weight of block",
      "    m_block = rho_wood * V_total",
      "    W_block = m_block * g",
      "    ",
      "    # For floating equilibrium: buoyant force = weight",
      "    # F_b = rho_water * V_submerged * g = W_block",
      "    V_submerged = W_block / (rho_water * g)",
      "    h_submerged = V_submerged / (length * width)",
      "    ",
      "    # Fraction submerged",
      "    fraction_submerged = V_submerged / V_total",
      "    ",
      "    # Theoretical fraction: rho_object / rho_fluid",
      "    fraction_theory = rho_wood / rho_water",
      "    ",
      "    print(f'  Block dimensions: {length}×{width}×{height} m³')",
      "    print(f'  Block density: {rho_wood} kg/m³')",
      "    print(f'  Water density: {rho_water} kg/m³')",
      "    print(f'  Volume submerged: {V_submerged:.3f} m³')",
      "    print(f'  Height submerged: {h_submerged:.3f} m')",
      "    print(f'  Fraction submerged: {fraction_submerged:.3f}')",
      "    print(f'  Theoretical fraction: {fraction_theory:.3f}')",
      "    print(f'  Agreement: {math.isclose(fraction_submerged, fraction_theory)}')",
      "    print()",
      "    ",
      "    return math.isclose(fraction_submerged, fraction_theory)",
      "",
      "# Test 2: Density measurement using buoyancy",
      "def verify_density_measurement():",
      "    \"\"\"Demonstrate density measurement using Archimedes principle\"\"\"",
      "    print('Test 2: Density measurement using buoyancy')",
      "    ",
      "    # Unknown object (aluminum)",
      "    rho_aluminum = 2700  # kg/m³ (actual value)",
      "    V_object = 0.001     # m³ (1 liter)",
      "    rho_water = 1000     # kg/m³",
      "    g = 9.81",
      "    ",
      "    # Measured quantities",
      "    W_air = rho_aluminum * V_object * g  # weight in air",
      "    F_buoyant = rho_water * V_object * g # buoyant force in water",
      "    W_water = W_air - F_buoyant          # apparent weight in water",
      "    ",
      "    # Calculate density from measurements",
      "    # rho_object = rho_water * W_air / (W_air - W_water)",
      "    rho_measured = rho_water * W_air / (W_air - W_water)",
      "    ",
      "    print(f'  Actual density (aluminum): {rho_aluminum} kg/m³')",
      "    print(f'  Measured density: {rho_measured:.0f} kg/m³')",
      "    print(f'  Measurement accuracy: {math.isclose(rho_measured, rho_aluminum)}')",
      "    print()",
      "    ",
      "    return math.isclose(rho_measured, rho_aluminum)",
      "",
      "# Run tests",
      "test1 = verify_floating_object()",
      "test2 = verify_density_measurement()",
      "",
      "print('Summary of Archimedes Principle Verifications:')",
      "print(f'Floating object equilibrium: {test1}')",
      "print(f'Density measurement: {test2}')",
      "print(f'All tests passed: {all([test1, test2])}')",
      "",
      "print('\\nArchimedes principle verified!')"
    ]
  },
