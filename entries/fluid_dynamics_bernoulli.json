      "    v1 = 2.0    # inlet velocity (m/s)",
      "    P1 = 200000 # inlet pressure (Pa)",
      "    ",
      "    # Continuity equation: A1*v1 = A2*v2",
      "    v2 = (A1 * v1) / A2",
      "    ",
      "    # Bernoulli's equation (assuming same height)",
      "    # P1 + (1/2)*rho*v1² = P2 + (1/2)*rho*v2²",
      "    P2 = P1 + 0.5 * rho * (v1**2 - v2**2)",
      "    ",
      "    # Dynamic pressures",
      "    q1 = 0.5 * rho * v1**2",
      "    q2 = 0.5 * rho * v2**2",
      "    ",
      "    # Total pressure (stagnation pressure)",
      "    P0_1 = P1 + q1",
      "    P0_2 = P2 + q2",
      "    ",
      "    print(f'  Inlet: A = {A1} m², v = {v1} m/s, P = {P1/1000:.0f} kPa')",
